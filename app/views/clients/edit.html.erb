<%= form_for(Client.find(params[:id]), url: {action: :update}) do |form| %>
    <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div class="field">
  <%= form.label :age %>
  <%= form.number_field :age %>
</div> 
<div class="field">
<%= form.label :address %>
<%= form.text_field :address %>
</div>
<div class="actions">
    <%= form.submit %>
  </div>

<% end %> 